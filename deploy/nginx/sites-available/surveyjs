server {
    listen 80;
    server_name srv03208.soton.ac.uk;

    location /favicon.ico {
        alias /var/www/rebooting-democracy-surveyjs-django/surveyjs/static/img/favicon.ico;
    }
    location /static/ {
        alias /var/www/rebooting-democracy-surveyjs-django/surveyjs/static/;
    }

    location / {
        include     uwsgi_params;
        uwsgi_pass  unix:/run/uwsgi/surveyjs.sock;
    }
}